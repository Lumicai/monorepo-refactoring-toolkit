{
  "extends": "@docusaurus/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@theme/*": ["./src/theme/*"],
      "@docusaurus/*": ["./node_modules/@docusaurus/*/lib/*"]
    }
  },
  "include": [
    "src/**/*",
    "scripts/**/*",
    "docusaurus.config.ts",
    "sidebars.ts",
    "sidebars-*.ts"
  ],
  "exclude": [
    "node_modules",
    "build",
    ".docusaurus"
  ]
}